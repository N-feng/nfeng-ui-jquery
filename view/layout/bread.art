<section class="content-header">
    <h1>
        {{h1}}
        <small>{{small}}</small>
    </h1>

    <ol class="breadcrumb">
        <li><a href="/public/index"><i class="fa fa-dashboard"></i> Home</a></li>
        {{each breadcrumb}} {{if $index === breadcrumb.length - 1}}
        <li class="active">{{$value}}</li> {{else}}
        <li><a href="/{{url.controller}}/{{url.action}}"> {{$value}}</a></li> {{/if}} {{/each}}
    </ol>
</section>